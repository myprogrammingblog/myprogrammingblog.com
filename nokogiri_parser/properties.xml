<?xml version="1.0" encoding="UTF-8"?>
<theme_package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../properties.xsd">
	<load_files>
		<javascripts>
			<!-- System frameworks are served directly from {un}roole -->
			<file>:system-framework/jquery</file>
			<file>example</file>
			<file>example2</file>
			<!-- Custom frameworks are added by the user -->
			<file>:custom-framework/bootstrap-3.1.1-dist/js/bootstrap.min</file>
			<file>example3</file>
			<file>example4</file>
			<file>:custom-framework/backbone/backbone</file>
		</javascripts>
		<stylesheets>
			<file>:custom-framework/bootstrap-3.1.1-dist/css/bootstrap</file>
			<file>sample1</file>
			<file>sample2</file>
			<file>:custom-framework/bootstrap-3.1.1-dist/css/bootstrap-theme</file>
			<file>sample3</file>
			<file>sample4</file>
		</stylesheets>
		<templates>
			<layouts>
				<layout>three-column</layout>
			</layouts>
			<pages>
				<page>about-me</page>
			</pages>
			<widgets>
				<widget>single_article_with_events</widget>
				<widget>gallery_with_events</widget>
			</widgets>
		</templates>
	</load_files>
	<system>
		<compatibility>
			<channel>website</channel>
		</compatibility>
	</system>
</theme_package>